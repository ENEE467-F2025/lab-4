<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro" name="robotiq_hande">

  <gazebo>
    <plugin filename="libgz_ros2_control-system.so" name="gz_ros2_control::GazeboSimROS2ControlPlugin">
      <parameters>$(find robotiq_hande_description)/config/ros2_controllers.yaml</parameters>
    </plugin>
  </gazebo>

  <link name="robotiq_hande_base_link" />

  <joint name="dummy" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <parent link="robotiq_hande_base_link"/>
    <child link="robotiq_hande_coupler"/>
  </joint>

  <link name="robotiq_hande_coupler">
    <visual>
      <geometry>
        <mesh filename="file://$(find robotiq_hande_description)/urdf/meshes/visual/io_coupler.dae"/>
      </geometry>
    </visual>

    <collision>
      <origin xyz="0 0 0.00545" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.0169" radius="0.0385"/>
      </geometry>
    </collision>

    <inertial>
      <mass value="0.86387"/>
      <origin xyz="0 0 0.00545" rpy="0 0 0"/>
      <inertia ixx="0.00101" ixy="0.0" ixz="0.0" iyy="0.00101" iyz="0.0" izz="0.000101"/>
    </inertial>
  </link>

  <joint name="robotiq_hande_base_joint" type="fixed">
    <origin xyz="0 0 0.011" rpy="0 0 0"/>
    <parent link="robotiq_hande_coupler"/>
    <child link="robotiq_hande_link"/>
  </joint>

  <link name="robotiq_hande_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="file://$(find robotiq_hande_description)/urdf/meshes/visual/hande.dae" scale="1.0 1.0 1.0"/>
      </geometry>
    </visual>

    <collision>
      <origin xyz="0 0 0.05345" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.1069" radius="0.0385"/>
      </geometry>
    </collision>

    <inertial>
      <mass value="0.86387"/>
      <origin xyz="0 0 0.00545" rpy="0 0 0"/>
      <inertia ixx="0.00101" ixy="0.0" ixz="0.0" iyy="0.00101" iyz="0.0" izz="0.000101"/>
    </inertial>
  </link>

  <joint name="robotiq_hande_left_finger_joint" type="prismatic">
    <origin xyz="0 0 0.099" rpy="0 0 0"/>
    <parent link="robotiq_hande_link"/>
    <child link="robotiq_hande_left_finger"/>
    <axis xyz="1 0 0"/>
    <limit effort="130" lower="0" upper="0.025" velocity="0.15"/>
  </joint>

  <link name="robotiq_hande_left_finger">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="file://$(find robotiq_hande_description)/urdf/meshes/visual/finger.dae" scale="1.0 1.0 1.0"/>
      </geometry>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="file://$(find robotiq_hande_description)/urdf/meshes/collision/finger_collision.dae" scale="1.0 1.0 1.0"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.03804"/>
      <inertia ixx="1E-9" ixy="0.0" ixz="0.0" iyy="1E-9" iyz="0.0" izz="1E-9"/>
    </inertial>
  </link>

  <joint name="robotiq_hande_right_finger_joint" type="prismatic">
    <origin xyz="0 0 0.099" rpy="0 0 0"/>
    <parent link="robotiq_hande_link"/>
    <child link="robotiq_hande_right_finger"/>
    <axis xyz="-1 0 0"/>
    <limit effort="130" lower="0" upper="0.025" velocity="0.15"/>
    <mimic joint="robotiq_hande_left_finger_joint" multiplier="1" offset="0"/>
  </joint>

  <link name="robotiq_hande_right_finger">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 ${pi}"/>
      <geometry>
        <mesh filename="file://$(find robotiq_hande_description)/urdf/meshes/visual/finger.dae" scale="1.0 1.0 1.0"/>
      </geometry>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 ${pi}"/>
      <geometry>
        <mesh filename="file://$(find robotiq_hande_description)/urdf/meshes/collision/finger_collision.dae" scale="1.0 1.0 1.0"/>
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.03804"/>
      <inertia ixx="1E-9" ixy="0.0" ixz="0.0" iyy="1E-9" iyz="0.0" izz="1E-9"/>
    </inertial>
  </link>

  <joint name="robotiq_hande_end_joint" type="fixed">
    <origin xyz="0 0 0.1455" rpy="0 0 0"/>
    <parent link="robotiq_hande_link"/>
    <child link="robotiq_hande_end"/>
  </joint>

  <link name="robotiq_hande_end"/>

</robot>